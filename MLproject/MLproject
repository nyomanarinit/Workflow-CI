name: churn-mlflow-project
conda_env: conda.yaml

entry_points:
  main:
    parameters:
      n_estimators: {type: int, default: 300}
      max_depth: {type: int, default: 20}
      dataset: {type: string, default: "churn_preprocessed.csv"}
    command: >
      python modelling.py {n_estimators} {max_depth} {dataset}
